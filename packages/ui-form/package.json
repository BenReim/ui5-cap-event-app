{
  "name": "ui5-cap-event-app-ui-form",
  "version": "0.0.0",
  "description": "A simple UI5 CAP Event Application Form",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "ui5 build --clean-dest",
    "start": "ui5 serve --port 8080 -o index.html",
    "lint": "npm run lint:eslint && npm run lint:ts-typecheck",
    "lint:eslint": "eslint webapp",
    "lint:ts-typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@sapui5/types": "1.116.0",
    "@types/dompurify": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@ui5/cli": "^3.4.0",
    "eslint": "^8.46.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "ui5-cap-event-app-server": "*",
    "ui5-middleware-cap": "^3.0.0",
    "ui5-middleware-livereload": "^3.0.0",
    "ui5-tooling-transpile": "^3.0.2"
  }
}
