{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
        "max-line-length": [
            true,
            160
        ],
        "object-literal-sort-keys": false,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-type"
        ]
    },
    "jsRules": {},
    "rulesDirectory": []
}